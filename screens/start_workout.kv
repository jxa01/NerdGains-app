<StartWorkoutScreen>:
	BoxLayout:
		orientation: 'vertical'
		spacing: 20
		canvas.before:
			Color:
				rgba: app.Colors['background']
			Rectangle:
				pos: self.pos
				size: self.size
		NavBar:
			MyDropDown:
				id: navigation
				text: "navigation"
				ddwidth: dp(130)
				values: ("History",)
				on_select: root.page_select(*args)
				
			DropButton:
				size_hint_x: None
				width: self.texture_size[0]+dp(25)
				text: "Start Empty Workout"
				on_release: app.root.current = 'exercise_selector'
				
		Widget:
		
		AnchorLayout:
			anchor_x: 'center'
			anchot_y: 'middle'
			MyLabel:
				text: "No Routines Available"
			
		Widget:
			